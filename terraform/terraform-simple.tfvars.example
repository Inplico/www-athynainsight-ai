# Example terraform.tfvars for Athyna Insight AI website
# Copy this to terraform.tfvars and fill in your values

# GCP Project Configuration
project_id = "your-gcp-project-id"
region     = "us-central1"
environment = "dev"

# Notification emails for monitoring alerts
notification_emails = ["admin@athynainsight.ai", "alerts@athynainsight.ai"]

